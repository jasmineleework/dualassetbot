# Frontend Environment Configuration Example
# Copy this file to .env and update with your settings

# Backend API URL - MUST match backend port (default: 8081)
REACT_APP_API_URL=http://localhost:8081

# Frontend Development Server Port (default: 3000)
# Use 3010 to avoid conflicts with other React apps
PORT=3010

# Application Version
REACT_APP_VERSION=1.0.0

# Environment (development/production)
REACT_APP_ENV=development

# Optional: Enable WebSocket auto-connect
# REACT_APP_AUTO_CONNECT_WS=false